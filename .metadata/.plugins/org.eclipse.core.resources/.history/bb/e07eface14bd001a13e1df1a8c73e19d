package 可口可乐销售;

public abstract class Builder {
	
	public abstract void getName(String name);
	public abstract void getAddress(String address);
	public abstract void getGoodsName(String goodsName);
	
	public abstract void setName();
	public abstract void setAddress();
	public abstract void setGoodsName(); //可口可乐或百事可乐
	public abstract void getResult();

}
